<div style="display: flex; justify-content: flex-end;">
    <a href="/api/session/logout"><button>Salir</button></a>
     <a href="/chat"><button>Chat</button></a>
</div>
<div>
    <h1>Tu perfil (sin datos sensibles)</h1>
    <p>{{user.name}}</p>
    <p>{{user.email}}</p>
    <p>{{user.age}}</p>
    {{#if user.githubProfile}}
    <p>Github data:</p>
    <div class="json-box">
        <pre>{{user.githubProfile}}</pre>
    </div>
    {{/if}}
</div>